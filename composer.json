{
    "name": "ambroisemaupate/intervention-request",
    "description":"A Intervention Image wrapper to use simple resample features over urls.",
    "license":"MIT",
    "type": "project",
    "keywords": [
        "image manipulation"
    ],
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@rezo-zero.com",
            "homepage": "http://www.rezo-zero.com",
            "role": "Lead developer"
        }
    ],
    "require": {
        "intervention/image": "~2.3",
        "symfony/http-foundation": "~2.8 || ~3.2",
        "symfony/console": "~2.8 || ~3.2",
        "symfony/finder": "~2.8 || ~3.2",
        "symfony/filesystem": "~2.8 || ~3.2",
        "symfony/event-dispatcher": "~2.8 || ~3.2",
        "symfony/process": "~2.8 || ~3.2",
        "monolog/monolog": "~1.22"
    },
    "require-dev": {
        "kraken-io/kraken-php": "^1.6"
    },
    "suggest": {
        "kraken-io/kraken-php": "Needed if you want to use the Kraken subscriber to send image to the kraken.io service."
    },
    "autoload": {
        "psr-4": {
            "AM\\InterventionRequest\\": "src/"
        }
    },
    "bin": ["bin/intervention", "bin/intervention.php"]
}
