{
    "name": "ambroisemaupate/intervention-request",
    "description":"A customizable Intervention Image wrapper to use simple image re-sampling features over urls and a configurable cache.",
    "license":"MIT",
    "type": "library",
    "keywords": [
        "image manipulation",
        "url",
        "resizing",
        "image cache",
        "php"
    ],
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@rezo-zero.com",
            "homepage": "http://www.rezo-zero.com",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-gd": "*",
        "intervention/image": "^2.4",
        "symfony/http-foundation": "^4.4",
        "symfony/console": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/process": "^4.4",
        "monolog/monolog": "^1.22",
        "kraken-io/kraken-php": "^1.6",
        "tinify/tinify": "^1.5",
        "ext-curl": "*"
    },
    "suggest" : {
        "ext-imagick": "Allow TIFF images to be implicitly converted to jpeg."
    },
    "autoload": {
        "psr-4": {
            "AM\\InterventionRequest\\": "src/"
        }
    },
    "bin": ["bin/intervention", "bin/intervention.php"],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    }
}
